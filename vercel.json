{
  "version": 2,
  "builds": [
    {
      "src": "src/mastra/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["node_modules/pino/**", "node_modules/@mastra/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/mastra/index.ts"
    }
  ]
}
